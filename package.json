{
  "name": "BackEnd",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "server": "nodemon index.js",
    "start": "node index.js",
    "test": "cross-env DB_ENV=testing jest ./tests --runInBand --detectOpenHandles --watch",
    "rollback-dev": "npx knex migrate:rollback",
    "latest-dev": "npx knex migrate:latest",
    "seed-dev": "npx knex seed:run",
